msgstr "Файл отвергнут"

#. translation for the 'instant' email interval option
#: pkg/store/defaults/templates.go:48
msgid "Instant"
msgstr "Сразу"

#. translation for the 'never' email interval option
#: pkg/store/defaults/templates.go:54
